{
  "name": "z-index-list",
  "version": "0.0.4",
  "author": "Jason Xie",
  "description": "Get a list of z-indexes used in your project",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/thejasonxie/z-index-list"
  },
  "keywords": [
    "z-index",
    "css",
    "html",
    "styles"
  ],
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "fast-glob": "^3.3.2"
  },
  "bin": {
    "z-index-list": "./dist/index.js"
  }
}
